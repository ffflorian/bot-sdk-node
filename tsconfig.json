{
  "compilerOptions": {
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es2016",
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "removeComments": true,
    "rootDir": "src",
    "sourceMap": true,
    "strict": true,
    "target": "ES5"
  }
}
